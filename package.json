{
  "name": "tigerpro",
  "version": "1.0.0",
  "description": "Tiger Pro App",
  "private": true,
  "scripts": {
    "checkFormat": "prettier --check \"wp-content/themes/tigerpro/frontend/src/**/*.{json,css,scss}\"",
    "format": "prettier --write \"wp-content/themes/tigerpro/frontend/src/**/*.{json,css,scss}\"",
    "lint": "npm run lint:js & npm run lint:css",
    "lint:js": "eslint wp-content/themes/tigerpro/frontend/src",
    "lint:css": "stylelint wp-content/themes/tigerpro/frontend/src/**/*.scss --fix --aei",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "start": "webpack serve --mode development --open",
    "prepare": "husky install"
  },
  "author": "Yevhenii Rodkin",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@wordpress/block-editor": "^6.1.0",
    "@wordpress/components": "^14.1.0",
    "@wordpress/element": "^3.1.0",
    "@wordpress/i18n": "^4.1.0",
    "autoprefixer": "^7.2.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concat": "^1.0.3",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^5.3.0",
    "prettier": "^2.3.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^11.1.1",
    "standard": "^16.0.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "standard": {
    "ignore": [
      "node_modules/**",
      "**/vendor/**.js"
    ]
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "@wordpress/blocks": "^9.1.0",
    "fitvids.1.1.0": "^1.1.0",
    "inputmask": "^5.0.5",
    "jquery": "^3.6.0",
    "jquery-validation": "^1.19.3",
    "magnific-popup": "^1.1.0",
    "slick-carousel": "^1.8.1",
    "validate": "^5.1.0"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ]
}
